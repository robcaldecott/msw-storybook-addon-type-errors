{
  "name": "msw-storybook-addon-close",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.1.0",
    "@mui/material": "^5.1.0",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@storybook/testing-react": "^1.2.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/css-mediaquery": "^0.1.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "cross-env": "^7.0.3",
    "css-mediaquery": "^0.1.2",
    "msw": "^0.36.0",
    "msw-storybook-addon": "^1.4.1",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.21.0",
    "react-query": "^3.32.1",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^4.0.3",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:coverage": "react-scripts test --passWithNoTests --coverage --watchAll=false",
    "test:ci": "cross-env CI=true yarn test:coverage -- --ci",
    "lint": "eslint src .storybook --max-warnings 0",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "format": "prettier --write src",
    "format:check": "prettier --check src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.ts*"
        ],
        "rules": {
          "no-duplicate-imports": "error"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
